name: "deepseek_merged"
backend: "python"
max_batch_size: 8

input [
  { name: "prompt", data_type: TYPE_STRING, dims: [ -1 ] }
]

output [
  { name: "text", data_type: TYPE_STRING, dims: [ -1 ] }
]

instance_group [
  { kind: KIND_GPU }
]

